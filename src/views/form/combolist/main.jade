.input
	input(type="text", data-view-id!="<%= viewId %>", placeholder!="<%= settings.placeholder %>")
	.caret
<% if (settings.editable) { %>
button.edit Edit
<% } %>
ul.list
ul.selected
	| <% selected.each(function(model) { %>
	li.selected(data-id!="<%= model.id %>") <%= model.get('title') %>
	| <% }); %>
